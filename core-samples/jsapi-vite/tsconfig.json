{
	"files": [],
	"references": [
		{ "path": "./tsconfig.app.json" },
	],
	"compilerOptions": {
		"target": "ES2023",
		"module": "ESNext",

		"skipLibCheck": true,
		"strict": false,
		"noUnusedLocals": true,
		"noUnusedParameters": true,
		"noFallthroughCasesInSwitch": true,
		"noEmit": true,
		"noUncheckedSideEffectImports": true,
		"moduleDetection": "force",
		"allowImportingTsExtensions": true,
		"verbatimModuleSyntax": true
	}
}
